<br>
<form action='/login' method='post'>
    Username: <input type="text" name='name'><br>
    Password: <input type="password" name='password'><br>
    <input type='submit' value='Log In'>
</form><br>

<%= session[:user_id]%>

<% if logged_in?%>
    <%= current_user.name%>
<% else%>
    <h3>No angler is logged in</h3>
<%end%>