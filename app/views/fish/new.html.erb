
<br>
<%= form_for @fish do |f|%>
 <%= render 'form'%>

    <%= f.label :weight%>
    <%= f.number_field :weight, step: :any%><br>
    <%= f.hidden_field :angler_id%>
    <%= f.collection_select :type_id, Type.all, :id, :name, include_blank: "Select"%><br>
    <%= f.fields_for :types, @fish.angler.types.build do |ff|%>
    <%= ff.label :name, 'New Type of Fish'%>
    <%= ff.text_field :name%><br>
    <%end%>
<%= f.submit "New Fish"%><br><br>
<%end%>



