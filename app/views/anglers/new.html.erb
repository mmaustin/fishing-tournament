<br>
<h3>Sign up below, Angler!</h3><br>

<%= form_for @angler do |f|%>
  <%= render 'form'%>
  <div class="new_form">
    <p class="new_p"><%= f.label :username%>:
    <%= f.text_field :username%></p>
    <p class="new-p"><%= f.label :email%>:
    <%= f.email_field :email%></p>
    <p class="new_p"><%= f.label :password%>:
    <%= f.password_field :password%></p>
    <p class="new_p"><%= f.label :hometown%>:
    <%= f.text_field :hometown%></p>
    <p class="new_p"><%= f.label :age%>:
    <%= f.number_field :age%></p>
    <p class="new_p"><%= f.label :catch_weight%>:
    <%= f.number_field :catch_weight, step: :any%></p><br>
    <p><%= f.submit 'Sign Up'%></p>
  </div>  
<%end%><br>


