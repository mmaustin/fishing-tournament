<br>
<p>Sign up below, Angler!</p><br>

<%= form_for @angler do |f|%>
  <div id="error_explanation">
    <h2><%= pluralize(@angler.errors.count, "error") %> prohibited this article from being saved:</h2>

    <ul>
      <% @angler.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>

    <%= f.label :username%>:
    <%= f.text_field :username%><br>
    <%= f.label :email%>:
    <%= f.email_field :email%><br>
    <%= f.label :password%>:
    <%= f.password_field :password%><br>
    <%= f.label :hometown%>:
    <%= f.text_field :hometown%><br>
    <%= f.label :age%>:
    <%= f.number_field :age%><br>
    <%= f.label :catch_weight%>:
    <%= f.number_field :catch_weight, step: :any%><br>
    <%= f.submit 'Sign Up'%>
<%end%><br>


