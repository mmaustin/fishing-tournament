<br>
<% flash.each do |type, msg| %>
  <div>
    <%= msg %>
  </div>
<% end %><br>

<h3>home</h3>
<% if !logged_in?%>
<%= link_to 'Sign Up', 'signup'%><br>
<%= link_to 'Log In', '/login'%><br>

<h3>Or log in through Facebook</h3>
<%= link_to('Log in with Facebook!', '/auth/facebook', method: :post) %><br>
<%else%>
        <%= link_to 'Your Page', angler_path(current_user)%>
        <%= link_to 'Log Out', '/logout'%><br>
<%end%>

<% if logged_in?%>
    <%= current_user.username%>
<% else%>
    <h3>No angler is logged in</h3>
<%end%>